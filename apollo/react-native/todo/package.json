{
  "name": "todosoffline",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "npm run cli -- start",
    "start-reset": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "debug": "react-native log-android",
    "relay:compile": "relay-compiler --src ./src --schema ./server/data/schema.graphql --language typescript",
    "test": "jest",
    "netinfo": "react-native --verbose link @react-native-community/netinfo",
    "async": "react-native link @react-native-community/async-storage",
    "icons": "react-native link react-native-vector-icons",
    "getIP": "node getIP.js",
    "server": "cd ./server && npm run start"
  },
  "dependencies": {
    "apollo-client": "^2.6.4",
    "react-apollo": "^3.1.3",
    "@react-native-community/netinfo": "5.7.1",
    "react": "16.11.0",
    "react-native": "0.62.1",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.5.0",
    "@react-native-community/async-storage": "^1.6.1",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-test-utils": "^0.1.1",
    "apollo-link-http": "1.5.15",
    "classnames": "2.2.6",
    "false": "0.0.4",
    "graphql": "^14.4.2",
    "graphql-tag": "2.10.1",
    "graphql-tools": "^0.10.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "subscriptions-transport-ws": "^0.9.16",
    "@wora/apollo-offline": "2.3.0",
    "@wora/detect-network": "2.0.0",
    "uuid": "3.3.2",
    "styled-components": "4.3.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "jest": "^25.2.7",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.11.0",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.15",
    "@types/react-native": "^0.57.51",
    "@types/react-relay": "^1.3.14",
    "@types/react-test-renderer": "^16.8.1",
    "@types/relay-runtime": "^1.3.12",
    "graphql": "^14.2.1",
    "ip": "1.1.5",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
